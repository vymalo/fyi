{
  "name": "@tegha-romeo/fyi-node",
  "version": "0.1.2",
  "description": "Node.js bindings for FYI",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "vym-fyi-node.linux-x64-gnu.node"
