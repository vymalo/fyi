  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tegha-romeo/fyi/tree/main/crates/vym-fyi-node"
  },
  "keywords": [
    "napi-rs",
    "node-addon",
    "rust"
  ],
  "author": "Tegha Romeo",
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^3.5.0"
  }
}
