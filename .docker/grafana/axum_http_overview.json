      "type": "timeseries",
      "title": "Total Request Rate (RPS)",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
